{
  "name": "daedalus",
  "productName": "Daedalus",
  "version": "0.8.0",
  "description": "Cryptocurrency Wallet",
  "main": "./dist/main/main.js",
  "scripts": {
    "preinstall": "which electron || npm install electron@1.7.9",
    "prepare": "patch-package",
    "lint": "eslint --format=node_modules/eslint-formatter-pretty app electron features *.js",
    "dev": "cross-env ELECTRON_WEBPACK_WDS_PORT=5000 npm run purge-translations && electron-webpack dev",
    "debug": "concurrently --kill-others \"npm run hot-server\" \"npm run start-debug-hot\" \"electron-inspector\"",
    "build": "electron-webpack",
    "package": "npm run build && cross-env NODE_ENV=production node -r babel-register -r babel-polyfill scripts/package.js",
    "package-all": "npm run package -- --all",
    "cleanup": "mop -v",
    "cucumber": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 cucumber-js --compiler js:babel-register --format 'progress-bar' --format-options '{\"snippetInterface\": \"synchronous\"}' --tags 'not @skip'",
    "cucumber-watch": "cross-env HOT=1 npm run cucumber -- --tags @watch",
    "test": "npm run cucumber --",
    "test-watch": "nodemon --exec cross-env ELECTRON_WEBPACK_WDS_PORT=5000 npm run cucumber-watch",
    "storybook": "start-storybook -p 6006 -c storybook",
    "build-storybook": "build-storybook",
    "flow-test": "flow; test $? -eq 0 -o $? -eq 2",
    "manage-translations": "node ./translations/translation-runner.js",
    "purge-translations": "rm -rf ./translations/messages/app",
    "postinstall": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json"
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "devDependencies": {
    "@storybook/addon-actions": "3.2.18",
    "@storybook/react": "3.2.18",
    "asar": "0.14.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-dev-expression": "0.2.1",
    "babel-plugin-lodash": "3.3.2",
    "babel-plugin-react-intl": "2.3.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-optimize": "1.0.1",
    "babel-register": "6.26.0",
