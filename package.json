    "mobx-react-form": "^1.18.16",
    "mobx-react-router": "^3.1.2",
    "moment": "^2.15.2",
    "numeral": "^2.0.4",
    "pdf.js-extract": "^0.0.5",
    "postcss": "^5.2.4",
    "prop-types": "^0.2.0",
    "qrcode.react": "^0.6.1",
